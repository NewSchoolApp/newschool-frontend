{
  "name": "newschool-frontend",
  "version": "1.0.0",
  "description": "New School Project",
  "author": "news-school-team",
  "private": true,
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "heroku-postbuild": "npm run build --prod",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest --coverage",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --ignore-path .gitignore",
    "*.vue": "eslint --ignore-path .gitignore"
  },
  "husky": {
      "hooks": {
        "pre-commit": "npm run lint-staged",
        "pre-push": "npm run test"
      }
  },
  "dependencies": {
    "@mdi/font": "^4.6.95",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/router-extras": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "fs": "0.0.1-security",
    "nuxt": "^2.0.0",
    "vue-avatar": "^2.1.8",
    "vue-social-sharing": "^2.4.7"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "husky": "^3.1.0",
    "jest": "^24.1.0",
    "lint-staged": "^9.5.0",
    "vue-jest": "^4.0.0-0"
  }
}
