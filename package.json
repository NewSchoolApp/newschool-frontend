{
  "name": "newschool-frontend",
  "version": "1.0.0",
  "description": "New School Project",
  "author": "news-school-team",
  "private": true,
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "heroku-postbuild": "npm run build --prod",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest --coverage",
    "lint-staged": "lint-staged",
    "cordova-ios": "npm run build-spa && npm run replace-relative-paths && npm run copy-cordova && npm run run-ios;",
    "cordova-android": "npm run build-spa && npm run replace-relative-paths && npm run copy-cordova && npm run run-android;",
    "build-spa": "nuxt build --spa",
    "replace-relative-paths": "replace '/nuxtfiles/' 'nuxtfiles/' dist --recursive && replace '/assets/' 'nuxtfiles/' dist --recursive && replace '(href|src|\"href\"|\"src\")=\"/' '$1=\"../' dist --recursive && replace '(href|src|\"href\"|\"src\"):\"/' '$1:\"../' dist --recursive",
    "copy-cordova": "cp -R dist/* cordova/www/",
    "clear-cordova": "rm -R cordova/www/*",
    "run-ios": "cd cordova && cordova run ios --buildFlag='-UseModernBuildSystem=0' --target='iPhone-X, 12.2'",
    "run-android": "cd cordova && cordova run android"
  },
  "lint-staged": {
    "*.js": "eslint --ignore-path .gitignore",
    "*.vue": "eslint --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "^4.6.95",
    "@nuxtjs/auth": "^4.8.5",
    "@nuxtjs/axios": "^5.9.3",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/router-extras": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "cordova-plugin-facebook4": "^6.2.0",
    "fs": "0.0.1-security",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "nuxt": "^2.11.0",
    "nuxt-i18n": "^6.4.1",
    "vue-avatar": "^2.1.8",
    "vue-social-sharing": "^2.4.7",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "vue-jest": "^4.0.0-0"
  }
}
