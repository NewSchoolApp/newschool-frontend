<widget id="com.newschool.app" version="2.0.5"  android-versionCode="10008" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
<name>New School</name>
<description> New School App </description>
<author email="newschool@http://newschool-api-dev2.eba-gxtzwa9m.us-east-2.elasticbeanstalk.com" href="http://cordova.io"> New School Team </author>
<content src="index.html"/>
<plugin name="cordova-plugin-whitelist" spec="1"/>
<plugin name="cordova-plugin-x-socialsharing" source="npm"/>
<plugin name="cordova-plugin-insomnia" version="4.1.0" source="npm" />
<access origin="*"/>
<allow-intent href="http://*/*"/>
<allow-intent href="https://*/*"/>
<allow-intent href="tel:*"/>
<allow-intent href="sms:*"/>
<allow-intent href="mailto:*"/>
<allow-intent href="geo:*"/>
<platform name="android">
<preference name="android-minSdkVersion" value="22"/>
<icon src="res/android/36px.png" density="ldpi" />
<icon src="res/android/48px.png" density="mdpi" />
<icon src="res/android/72px.png" density="hdpi" />
<icon src="res/android/96px.png" density="xhdpi" />
<icon src="res/android/144px.png" density="xxhdpi" />
<icon src="res/android/192px.png" density="xxxhdpi" />
<allow-intent href="market:*"/>
</platform>
<platform name="ios">
<preference name="prerendered-icon" value="true" />
<!-- iOS 8.0+ -->
<!-- iPhone 6 Plus  -->
<icon src="res/ios/Icon-App-60x60@3x.png" width="180" height="180" />
<!-- Spotlight Icon -->
<icon src="res/ios/Icon-App-40x40@1x.png" width="40" height="40" />
<icon src="res/ios/Icon-App-40x40@2x.png" width="80" height="80" />
<!-- iOS 7.0+ -->
<!-- iPhone / iPod Touch  -->
<icon src="res/ios/Icon-App-60x60@2x.png" width="120" height="120" />
<!-- iPad -->
<icon src="res/ios/Icon-App-76x76@1x.png" width="76" height="76" />
<icon src="res/ios/Icon-App-76x76@2x.png" width="152" height="152" />
<!-- iOS 6.1 -->
<!-- iPhone / iPod Touch -->
<icon src="res/ios/Icon-App-29x29@2x.png" width="57" height="57" />
<icon src="res/ios/Icon-App-29x29@3x.png" width="114" height="114" />
<!-- iPad -->
<icon src="res/ios/Icon-App-76x76@1x.png" width="72" height="72" />
<icon src="res/ios/Icon-App-76x76@2x.png" width="144" height="144" />
<!-- iPad Pro -->
<icon src="res/ios/Icon-App-83.5x83.5@2x.png" width="167" height="167" />
<!-- iPad Pro -->
<icon src="res/ios/Icon-App-83.5x83.5@2x.png" width="167" height="167" />
<splash src="res/screen/ios/Default@2x~iphone~anyany.png" />
<splash src="res/screen/ios/Default@2x~ipad~anyany.png" />
<splash src="res/screen/ios/Default@2x~iphone~comany.png" />
<splash src="res/screen/ios/Default@2x~ipad~comany.png" />
<splash src="res/screen/ios/Default@3x~iphone~anyany.png" />
<splash src="res/screen/ios/Default@3x~iphone~comany.png" />
<preference name="StatusBarOverlaysWebView" value="false" />
<preference name="StatusBarBackgroundColor" value="#000000" />
<preference name="StatusBarStyle" value="lightcontent" />
<preference name="DisallowOverscroll" value="true" />
<plugin spec="https://github.com/apache/cordova-plugin-statusbar.git" source="git" />
<allow-intent href="itms:*"/>
<allow-intent href="itms-apps:*"/>
<allow-navigation href="https://*youtube.com/*"/>
</platform>
</widget>
