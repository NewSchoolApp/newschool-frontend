<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="10015" id="com.newschool.app" version="2.0.12" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NewSchool</name>
    <description> New School App </description>
    <author email="newschool@http://newschool-api-dev2.eba-gxtzwa9m.us-east-2.elasticbeanstalk.com" href="http://cordova.io"> New School Team </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-x-socialsharing" source="npm" />
    <plugin name="cordova-plugin-insomnia" source="npm" version="4.1.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="22" />
        <icon density="ldpi" src="res/android/36px.png" />
        <icon density="mdpi" src="res/android/48px.png" />
        <icon density="hdpi" src="res/android/72px.png" />
        <icon density="xhdpi" src="res/android/96px.png" />
        <icon density="xxhdpi" src="res/android/144px.png" />
        <icon density="xxxhdpi" src="res/android/192px.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <preference name="prerendered-icon" value="true" />
        <icon height="20" src="res/ios/Icon-App-20x20@1x.png" width="20" />
        <icon height="29" src="res/ios/Icon-App-29x29@1x.png" width="29" />
        <icon height="58" src="res/ios/Icon-App-29x29@2x.png" width="58" />
        <icon height="87" src="res/ios/Icon-App-29x29@3x.png" width="87" />
        <icon height="40" src="res/ios/Icon-App-60x60@1x.png" width="60" />
        <icon height="180" src="res/ios/Icon-App-60x60@3x.png" width="180" />
        <icon height="40" src="res/ios/Icon-App-40x40@1x.png" width="40" />
        <icon height="80" src="res/ios/Icon-App-40x40@2x.png" width="80" />
        <icon height="120" src="res/ios/Icon-App-60x60@2x.png" width="120" />
        <icon height="76" src="res/ios/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="res/ios/Icon-App-76x76@2x.png" width="152" />
        <icon height="57" src="res/ios/Icon-App-29x29@2x.png" width="57" />
        <icon height="114" src="res/ios/Icon-App-29x29@3x.png" width="114" />
        <icon height="72" src="res/ios/Icon-App-76x76@1x.png" width="72" />
        <icon height="144" src="res/ios/Icon-App-76x76@2x.png" width="144" />
        <icon height="167" src="res/ios/Icon-App-83.5x83.5@2x.png" width="167" />
        <icon height="167" src="res/ios/Icon-App-83.5x83.5@2x.png" width="167" />
        <icon height="1024" src="res/ios/ItunesArtwork@2x.png" width="1024" />
        <splash src="res/screen/ios/Default@2x~iphone~anyany.png" />
        <splash src="res/screen/ios/Default@2x~ipad~anyany.png" />
        <splash src="res/screen/ios/Default@2x~iphone~comany.png" />
        <splash src="res/screen/ios/Default@2x~ipad~comany.png" />
        <splash src="res/screen/ios/Default@3x~iphone~anyany.png" />
        <splash src="res/screen/ios/Default@3x~iphone~comany.png" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#000000" />
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="DisallowOverscroll" value="true" />
        <plugin source="git" spec="https://github.com/apache/cordova-plugin-statusbar.git" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <allow-navigation href="https://*youtube.com/*" />
    </platform>
    <engine name="android" />
</widget>
